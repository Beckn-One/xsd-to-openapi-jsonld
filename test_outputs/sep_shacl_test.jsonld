        {
          "sh:path": {
            "@id": "https://schemas.ieee.org/2030.5/TariffProfileListLink"
          },
          "sh:maxCount": 1,
          "sh:datatype": "xsd:string"
        },
        {
          "sh:path": {
            "@id": "https://schemas.ieee.org/2030.5/TimeLink"
          },
          "sh:maxCount": 1,
          "sh:datatype": "xsd:string"
        },
        {
          "sh:path": {
            "@id": "https://schemas.ieee.org/2030.5/UsagePointListLink"
          },
          "sh:maxCount": 1,
          "sh:datatype": "xsd:string"
        }
      ]
    },
    {
      "@id": "https://schemas.ieee.org/2030.5/FunctionSetAssignmentsShape",
      "@type": "sh:NodeShape",
      "sh:targetClass": {
        "@id": "https://schemas.ieee.org/2030.5/FunctionSetAssignments"
      },
      "rdfs:comment": "Provides an identifiable, subscribable collection of resources for a particular device to consume.",
      "sh:property": [
        {
          "sh:path": {
            "@id": "https://schemas.ieee.org/2030.5/mRID"
          },
          "sh:minCount": 1,
          "sh:maxCount": 1,
          "sh:datatype": "xsd:string",
          "rdfs:comment": "The global identifier of the object."
        },
        {
          "sh:path": {
            "@id": "https://schemas.ieee.org/2030.5/description"
          },
          "sh:maxCount": 1,
          "sh:datatype": "xsd:string",
          "rdfs:comment": "The description is a human readable text describing or naming the object."
        },
        {
          "sh:path": {
            "@id": "https://schemas.ieee.org/2030.5/version"
          },
          "sh:maxCount": 1,
          "sh:minInclusive": 0,
          "sh:maxInclusive": 65535,
          "sh:datatype": "xsd:integer",
          "rdfs:comment": "Contains the version number of the object. See the type definition for details."
        },
        {
          "sh:path": {
            "@id": "https://schemas.ieee.org/2030.5/subscribable"
