      "sh:targetClass": {
        "@id": "https://schemas.ieee.org/2030.5/DeviceCapability"
      },
      "rdfs:comment": "Returned by the URI provided by DNS-SD, to allow clients to find the URIs to the resources in which they are interested.",
      "sh:property": [
        {
          "sh:path": {
            "@id": "https://schemas.ieee.org/2030.5/EndDeviceListLink"
          },
          "sh:maxCount": 1,
          "sh:datatype": "xsd:string"
        },
        {
          "sh:path": {
            "@id": "https://schemas.ieee.org/2030.5/MirrorUsagePointListLink"
          },
          "sh:maxCount": 1,
          "sh:datatype": "xsd:string"
        },
        {
          "sh:path": {
            "@id": "https://schemas.ieee.org/2030.5/SelfDeviceLink"
          },
          "sh:maxCount": 1,
          "sh:datatype": "xsd:string"
        },
        {
          "sh:path": {
            "@id": "https://schemas.ieee.org/2030.5/pollRate"
          },
          "sh:maxCount": 1,
          "sh:minInclusive": 0,
          "sh:maxInclusive": 4294967295,
          "sh:datatype": "xsd:unsignedInt",
